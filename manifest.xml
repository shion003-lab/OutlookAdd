<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>12345678-90ab-cdef-1234-567890abcdef</Id>
  <Version>1.0.0.3</Version>
  <ProviderName>Openloop開発</ProviderName>
  <DefaultLocale>ja-JP</DefaultLocale>
  <DisplayName DefaultValue="問い合わせ管理"/>
  <Description DefaultValue="ローカルサーバーで動作するOutlook Web アドイン"/>
  <SupportUrl DefaultValue="http://localhost:8000/"/>
  <AppDomains>
    <AppDomain>http://localhost:8000</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="Mailbox"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://localhost:4443/taskpane.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
</OfficeApp>
